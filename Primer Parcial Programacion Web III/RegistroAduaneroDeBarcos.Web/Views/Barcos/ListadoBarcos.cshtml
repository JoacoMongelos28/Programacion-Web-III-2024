@model List<BarcoModel>

<h1>Lista de Barcos</h1>
<a href="/Barcos/RegistrarBarco">Registrar Barco</a>

<table class="table table-striped">
    <thead>
        <tr>
            <th>#Id Barco</th>
            <th>Nombre</th>
            <th>Antiguedad</th>
            <th>Tripulacion Maxima</th>
            <th>Tasa</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var barco in Model)
        {
            <tr>
                <td>@barco.IdBarco</td>
                <td>@barco.Nombre</td>
                <td>@barco.Antiguedad</td>
                <td>@barco.TripulacionMaxima</td>
                <td>@barco.Tasa</td>
            </tr>
        }
    </tbody>
</table>